<Project>
  <Import Project="$([MSBuild]::GetPathOfFileAbove('Directory.Build.props', '$(MSBuildThisFileDirectory)../'))" />

  <PropertyGroup>
    <TargetFrameworks>net452;net461;netcoreapp2.1</TargetFrameworks>
    <ExcludeNativeProfiler>true</ExcludeNativeProfiler>
  </PropertyGroup>

  <!--  Move bin and obj folders to root -->
  <PropertyGroup>
    <BaseOutputPath>$(SolutionDir)bin\bin\test\test-applications\regression\dependency-libs\$(MSBuildProjectName)</BaseOutputPath>
    <BaseIntermediateOutputPath>$(SolutionDir)bin\obj\test\test-applications\regression\dependency-libs\$(MSBuildProjectName)</BaseIntermediateOutputPath>
  </PropertyGroup>
</Project>