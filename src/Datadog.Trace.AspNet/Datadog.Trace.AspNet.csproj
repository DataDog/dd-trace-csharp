<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net45</TargetFrameworks>

    <!-- NuGet -->
    <Version>1.0</Version>
    <Title>Datadog APM Tracing for ASP.NET</Title>
    <Description>Provides tracing for ASP.NET web applications.</Description>
  </PropertyGroup>

  <ItemGroup>
    <ProjectReference Include="..\Datadog.Trace\Datadog.Trace.csproj" />

    <Reference Include="System.Web" />

    <!-- reference assemblies let us build with the .NET Framework SDK installed -->
    <PackageReference Include="Microsoft.NETFramework.ReferenceAssemblies" Version="1.0.0" PrivateAssets="all" />
  </ItemGroup>

</Project>