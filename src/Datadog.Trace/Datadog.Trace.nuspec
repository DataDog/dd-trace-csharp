<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2012/06/nuspec.xsd">
  <metadata>
    <id>Datadog.Trace</id>
    <version>1.7.0</version>
    <title>Datadog APM</title>
    <authors>lucas.pimentel.datadog,colinhigginsdatadog,zachmontoyadd</authors>
    <owners>lucas.pimentel.datadog,colinhigginsdatadog,zachmontoyadd</owners>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <license type="expression">Apache-2.0</license>
    <projectUrl>https://docs.datadoghq.com/tracing/setup/dotnet/</projectUrl>
    <iconUrl>https://github.com/DataDog/dd-trace-dotnet/raw/master/datadog-logo-64x64.png</iconUrl>
    <description>Manual instrumentation library for Datadog APM</description>
    <releaseNotes>See release notes at https://github.com/DataDog/dd-trace-dotnet/releases</releaseNotes>
    <copyright>Datadog, Inc. 2017-2019</copyright>
    <tags>Datadog APM tracing profiling instrumentation</tags>
    <repository type="git" url="https://github.com/DataDog/dd-trace-dotnet.git" />
    <dependencies>
      <group targetFramework=".NETFramework4.5">
        <dependency id="MsgPack.Cli" version="1.0.1" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="12.0.1" exclude="Build,Analyzers" />
        <dependency id="System.Net.Http" version="4.3.4" exclude="Build,Analyzers" />
        <dependency id="System.Runtime.InteropServices.RuntimeInformation" version="4.3.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETFramework4.6.1">
        <dependency id="MsgPack.Cli" version="1.0.1" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="12.0.1" exclude="Build,Analyzers" />
        <dependency id="System.Net.Http" version="4.3.4" exclude="Build,Analyzers" />
        <dependency id="System.Runtime.InteropServices.RuntimeInformation" version="4.3.0" exclude="Build,Analyzers" />
      </group>
      <group targetFramework=".NETStandard2.0">
        <dependency id="MsgPack.Cli" version="1.0.1" exclude="Build,Analyzers" />
        <dependency id="Newtonsoft.Json" version="12.0.1" exclude="Build,Analyzers" />
        <dependency id="System.Net.Http" version="4.3.4" exclude="Build,Analyzers" />
        <dependency id="System.Runtime.InteropServices.RuntimeInformation" version="4.3.0" exclude="Build,Analyzers" />
      </group>
    </dependencies>
    <frameworkAssemblies>
      <frameworkAssembly assemblyName="System.Configuration" targetFramework=".NETFramework4.50.0" />
      <frameworkAssembly assemblyName="System.Web" targetFramework=".NETFramework4.50.0" />
    </frameworkAssemblies>
  </metadata>
